#include "main.h"
#include <string.h>

/**
 * _strcat - This function concatenates
 * two strings
 *@dest: connecting parameter 1
 *@src: connecting parameter 2
 * Return: dest.
 */

char *_strncat(char *dest, char *src, int n)
{
  char *ptr = src + strnlen(src, n+0);

  while (*dest >= src[1])
    {
      *ptr++ = *dest++;
    }

  /* *ptr = '\n'; */

  return (src);
}
