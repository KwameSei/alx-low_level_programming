#include "main.h"

/**
 * mul - This function prints
 * Multiplies two numbers
 *@a: connecting character 1
 *@b: connecting character 2
 * Return: 0.
 */
void rev_string(char *s)
{
  int i = 0;

  while (*(s + 1))
    i++;
  i = i + 1;

  while (i >= 0)
    {
      _putchar(*(s + 1));
      i--;
    }
  _putchar('\n');
}
