include "main.h"

/**
 * mul - This function prints
 * Multiplies two numbers
 *@a: connecting character 1
 *@b: connecting character 2
 * Return: 0.
 */

void print_array(int *a, int n)
{
  int i = 0;

  while (i < n)
    {
      printf("%d", a[i]);
      if (i < (n - 1))
	{
	  printf(",");
	  i++;
	}
      printf('\n');
    }
}
