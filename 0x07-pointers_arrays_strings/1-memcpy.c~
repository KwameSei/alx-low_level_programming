#include "main.h"

/**
 * _memset - This function fills
 * the first n bytes of the memory area.
 *@s: connecting character 1
 *@b: connecting character 2
 *@n: connecting character 3
 * Return: 0.
 */

char *_memcpy(char *dest, char *src, unsigned int n)
{
  unsigned int i;

  for (i = 0; i < n; i++)
    {
      *(dest + i) = *(src + i);
    }
  return (s);
}
