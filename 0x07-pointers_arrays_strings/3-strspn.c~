#include "main.h"

/**
 * _strchr - This function copies
 * the first n bytes of the memory area.
 *@s: connecting character 1
 *@c: connecting character 2
 * Return: 0.
 */

unsigned int _strspn(char *s, char *accept)
{
  unsigned int i = 0, j, t = 0;

  while (accept[i])
    {
      j = 0;

      while (s[j] != 32)
	{
	  if (accept[i] == s[j])
	    {
	      k++;
	    }
	  j++;
	}
      i++;
    }
  return (k);
}
