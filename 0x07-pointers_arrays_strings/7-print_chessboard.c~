#include "main.h"

/**
 * _strstr - This function locates
 * a substring
 *@haystack: connecting character 1
 *@needle: connecting character 2
 * Return: 0.
 */
void print_chessboard(char (*a)[8])
{
  int b, c;

  for (b = 0; b < 8; b++)
    {
      for (c = 0; c < 8; c++)
	{
	  _putchar(a[b][c]);
	}
      _putchar('\n');
    }
}
