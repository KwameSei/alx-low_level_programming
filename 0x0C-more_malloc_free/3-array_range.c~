#include "main.h"

/**
 * mul - This function prints
 * Multiplies two numbers
 *@a: connecting character 1
 *@b: connecting character 2
 * Return: 0.
 */

int *array_range(int min, int max)
{
  int *ptr;
  int a;

  if (min > max)
    return (NULL);

  ptr = malloc(sizeof(*ptr) * ((max - min) + 1))

    if (ptr == NULL)
      return (NULL);

  for (a = 0; min <= max; a++, min++)
    ptr[a] = min;
  
  return (ptr);
}
