/**
 *_strdup - Entry point
 *@str: size of the array
 *Return: copied string
 */

char *str_concat(char *s1, char *s2)
{
  char *str;
  unsigned int a, b, c, limit;

  if (s1 == NULL)
    s1 = "";
  if (s2 == NULL)
    s2 = "";

  for (a = 0; s1[a] != '\0'; a++)
    ;

  for (b = 0; b[s2] != '\0'; b++)

    str = malloc(sizeof(char) * (a + b + 1));

